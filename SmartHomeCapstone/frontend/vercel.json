{
  "name": "smart-home-frontend",
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
